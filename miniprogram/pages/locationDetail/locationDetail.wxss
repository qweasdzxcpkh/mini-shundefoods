/* miniprogram/pages/locationDetail/locationDetail.wxss */
page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.location-container {
  /* width: 100%; */
  /* height: 800rpx; */
  padding: 20rpx 0rpx 20rpx 0;
  display: flex;
  flex-direction: column;
  align-content: center;
  /* justify-content: center; */
  box-sizing: border-box;
  background-color: white;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  margin-left: 15rpx;
  margin-right: 15rpx;
}

.location-detail {
  /* margin-top: 40rpx; */
  height: 70rpx;
  width: 100%;
  border-left: none;
  border-right: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
  /* background-color: aliceblue; */
}

.location-content {

}

.location-per-cost {
  color: darkgrey;
  font-size: 30rpx;
}
.location-per-cost:before {
  content: "¥";
}
.location-per-cost:after {
  content: "/人";
  font-size: 22rpx;
}

.location-recommend-foods {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: row;
  font-size: 30rpx;
  color: darkgrey;
}

.location-recommend {
  width: 100%;
  height: auto;
  white-space: nowrap;
  display: flex;
  flex-direction: row;
  /* flex-wrap: nowrap; */
  /* align-items: center; */
  /* overflow: hidden; */
}
.location-recommend-image {
  border-radius: 10%;
  margin: 0 10rpx 0 10rpx;
  overflow: hidden;
}

.location-address {
  height: 100rpx;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  padding: 20rpx 20rpx 20rpx 20rpx;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  display: flex;
  flex-direction: row;
  align-items: center;
}

.location-address-left {
  width: 70%;
  height: 100%;
  text-align: left;
  align-items: center;
  align-content: center;
  justify-content: center;
  border-right: 1px solid rgba(0, 0, 0, 0.1);
}
