.bussiness-time {
  height: auto;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  padding: 20rpx 20rpx 20rpx 20rpx;
  font-size: 30rpx;
  /* border-top: 1px solid rgba(0, 0, 0, 0.1); */
  /* border-bottom: 1px solid rgba(0, 0, 0, 0.1); */
  display: flex;
  flex-direction: column;
  /* align-items: center; */
}

.bottom-tab {
  align-items: center;
  display: flex;
  flex-direction: row;
	position: fixed;
	bottom: 0;
	width: 100%;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
}

